<?xml version= "1.0" encoding= "utf-8" ?>

<AbsoluteLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsLay1">

    <ImageView
        android:layout_width="104dp"
        android:layout_height="104dp"
        android:src="@drawable/logo1"
        android:layout_x="106dp"
        android:layout_y="20dp" />
    <Button
        android:layout_width="68dp"
        android:layout_height="65dp"
        android:id="@+id/button2"
        android:background="@drawable/user1"
        android:layout_x="40dp"
        android:layout_y="155dp"
        android:onClick="lanzarLogin"/>

    <Button
        android:layout_width="56dp"
        android:layout_height="62dp"
        android:background="@drawable/menu"
        android:id="@+id/button3"
        android:layout_x="190dp"
        android:layout_y="156dp"
        android:onClick="lanzarMenu"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOGUEARSE"
        android:layout_x="30dp"
        android:layout_y="230dp"
        android:id="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MENU"
        android:layout_x="195dp"
        android:layout_y="230dp"
        android:id="@+id/textView6" />


    <Button
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView3"
        android:background="@drawable/acerca"
        android:layout_x="130dp"
        android:layout_y="269dp"
        android:onClick="lanzarInformacion"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INFORMACION"
        android:layout_x="110dp"
        android:layout_y="322dp" />


</AbsoluteLayout>